Options +FollowSymlinks
RewriteEngine on
#DirectoryIndex  index.php
RewriteBase /
RewriteCond %{REQUEST_URI} !/blog/
RewriteCond %{REQUEST_URI} !/control-block/
RewriteCond %{REQUEST_URI} !/support/
RewriteCond %{REQUEST_URI} !/support/admin/
RewriteCond %{REQUEST_URI} !\.(?:css|js|jpeg|gif|png|woff|eot|svg|ttf)$ [NC]
RewriteCond %{REQUEST_URI} !(/en/|/sa/|/ae/|/eg/|/qa/|/kw/)
RewriteCond %{REQUEST_URI} !/public
RewriteCond %{REQUEST_URI} !/public/popshow
RewriteCond %{REQUEST_URI} !(index.php)
RewriteRule https://www.pandacashback.com/urlrewrite.php?url=%{REQUEST_URI} [L,NC,QSA]

RewriteCond %{REQUEST_URI} !\.(?:css|js|jpe?g|gif|png|woff|eot|svg|ttf)$ [NC]
RewriteCond %{REQUEST_URI} (/en/) [NC]
RewriteRule en/?(.*)$ public/$1 [L,NC,QSA]

RewriteCond %{REQUEST_URI} !\.(?:css|js|jpe?g|gif|png|woff|eot|svg|ttf)$ [NC]
RewriteCond %{REQUEST_URI} (/sa/) [NC]
RewriteRule sa/?(.*)$ public/$1 [L,NC,QSA]

RewriteCond %{REQUEST_URI} !\.(?:css|js|jpe?g|gif|png|woff|eot|svg|ttf)$ [NC]
RewriteCond %{REQUEST_URI} (/ae/) [NC]
RewriteRule ae/?(.*)$ public/$1 [L,NC,QSA]

RewriteCond %{REQUEST_URI} !\.(?:css|js|jpe?g|gif|png|woff|eot|svg|ttf)$ [NC]
RewriteCond %{REQUEST_URI} (/eg/) [NC]
RewriteRule eg/?(.*)$ public/$1 [L,NC,QSA]

RewriteCond %{REQUEST_URI} !\.(?:css|js|jpe?g|gif|png|woff|eot|svg|ttf)$ [NC]
RewriteCond %{REQUEST_URI} (/qa/) [NC]
RewriteRule qa/?(.*)$ public/$1 [L,NC,QSA]

RewriteCond %{REQUEST_URI} !\.(?:css|js|jpe?g|gif|png|woff|eot|svg|ttf)$ [NC]
RewriteCond %{REQUEST_URI} (/kw/) [NC]
RewriteRule kw/?(.*)$ public/$1 [L,NC,QSA]



<IfModule mod_rewrite.c>

DirectoryIndex index.php
</IfModule>
AddHandler application/x-httpd-php56 .php .php5 .php4 .php3
<Files 403.shtml>
order allow,deny
allow from all
</Files>
